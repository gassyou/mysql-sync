{
  "name": "mysql-sync",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron:tsc": "tsc main.ts",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:tsc && electron ./ --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && npx electron-builder build --linux",
    "electron:windows": "npm run build:prod && npx electron-builder build --windows",
    "electron:mac": "npm run build:prod && npx electron-builder build --mac",
    "start": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "ng:serve": "ng serve",
    "ng:serve:web": "npm run postinstall:web && npm run electron:tsc  && ng serve -o",
    "postinstall": "npm run postinstall:electron && npx electron-builder install-app-deps",
    "postinstall:web": "node postinstall-web",
    "postinstall:electron": "node postinstall",
    "start:electron": "ng build --base-href ./ && electron .",
    "build:electron": "mkdirp ./bin/package && ng build --outputPath=bin/package/dist --prod --base-href && cpx main.js ./bin/package && cpx package.json ./bin/package && rimraf ./bin/resources/app.asar &&  asar pack ./bin/package ./bin/resources/app.asar && rimraf ./bin/package"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.1.2",
    "@angular/common": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/core": "~8.1.2",
    "@angular/forms": "~8.1.2",
    "@angular/platform-browser": "~8.1.2",
    "@angular/platform-browser-dynamic": "~8.1.2",
    "@angular/router": "~8.1.2",
    "mysql": "^2.15.0",
    "ng-zorro-antd": "^8.5.2",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "typescript-string-operations": "^1.3.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.2",
    "@angular/cli": "~8.1.2",
    "@angular/compiler-cli": "~8.1.2",
    "@angular/language-service": "~8.1.2",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/mysql": "^2.15.8",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3",
    "electron": "^5.0.4",
    "electron-builder": "^20.43.0",
    "electron-packager": "^13.1.1",
    "electron-reload": "^1.4.0",
    "fs": "0.0.1-security",
    "angular-cli-builders": "^2.1.2",
    "commonjs": "0.0.1",
    "copy-webpack-plugin": "^5.0.3",
    "core-js": "^2.6.5",
    "npm-run-all": "4.1.5",
    "npx": "^10.2.0",
    "rxjs": "^6.5.2",
    "stream": "0.0.2",
    "wait-on": "3.2.0",
    "zone.js": "^0.8.29"
  }
}
